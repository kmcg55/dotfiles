# Aliases
alias home='cd ~'
alias root='cd /'
alias dtop='cd $HOME/Desktop'
alias projects='cd $HOME/Documents/dev/projects'
alias notes='cd $HOME/Documents/Notes'
alias docs='cd $HOME/Documents'
alias todo='$EDITOR $HOME/todo.txt'
alias o=open
alias q='exit'
alias c='clear'
alias cs='clear; ls'
alias lsa='ls -a'
alias lsl='ls -l'
alias t='time'
alias k='kill'
alias bashrc='$EDITOR $HOME/.bashrc'
alias vimrc='$EDITOR $HOME/.vimrc'
alias zshrc='$EDITOR $HOME/.zshrc'
# Add path for hifi-rs
alias hifi-rs='$HOME/Documents/dev/projects/hifi.rs/target/x86_64-apple-darwin/release/hifi-rs'
alias hifi='hifi-rs open'
