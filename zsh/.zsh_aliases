# Aliases
alias home='cd ~'
alias root='cd /'
alias dtop='cd ~/Desktop'
alias projects='cd ~/Documents/dev/projects'
alias o=open
alias q='exit'
alias c='clear'
alias cs='clear; ls'
alias lsa='ls -a'
alias lsl='ls -l'
alias t='time'
alias k='kill'
alias bashrc='vim ~/.bashrc'
alias vimrc='vim ~/.vimrc'
alias zshrc='vim ~/.zshrc'
